{
  "name": "pg-pilot",
  "version": "0.1.0",
  "description": "Manage your PostgreSQL databases in the browser",
  "main": "index.js",
  "preferGlobal": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover _mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "back": "node server.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saidM/pg-pilot.git"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "database",
    "db",
    "sql"
  ],
  "author": "Sa√Ød Mimouni <sayid.mimouni@gmail.com>",
  "license": "ISC",
  "bin": {
    "pg-pilot": "./bin/pg-pilot.js"
  },
  "bugs": {
    "url": "https://github.com/saidM/pg-pilot/issues"
  },
  "homepage": "https://localhost:9000/",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "react-scripts": "0.7.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "minimist": "^1.2.0",
    "pg": "^6.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-scripts": "^0.7.0"
  }
}
